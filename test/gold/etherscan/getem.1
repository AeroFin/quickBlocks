./getem.2 $1 $2

echo $1 | sed 's/0x//' >y
cat y | sed 's/^/s\/0x\//' | sed 's/$/\//' >z

./colorEcho -t "Diffing for in new gold, not in old gold: $2-$1"
diff --suppress-common-lines -y ~/src.GitHub/quickBlocks/test/gold/monitors/txt/$3$2-$1.txt txt/$2-$1.txt | grep "<" | cut -c1-20 >crap
cat crap | cut -f1,2 | sed 's/$/ 0x/' | sed -f z | sed 's/^/.\/getem.3 /' >x ; chmod uog+x x ; ./x | tee results/$1_qb_not_es.txt

./colorEcho -t "Diffing for in old gold, not in new gold: $2-$1"
diff --suppress-common-lines -y ~/src.GitHub/quickBlocks/test/gold/monitors/txt/$3$2-$1.txt txt/$2-$1.txt | grep ">" | cut -c15-100 >crap
cat crap | cut -f2,3 | sed 's/$/ 0x/' | sed -f z | sed 's/^/.\/getem.3 /' >x ; chmod uog+x x ; ./x | tee results/$1_es_not_qb.txt

./colorEcho -t "Diffing for changes: $2-$1"
diff --suppress-common-lines -y ~/src.GitHub/quickBlocks/test/gold/monitors/txt/$3$2-$1.txt txt/$2-$1.txt | grep "|" | tee results/$1_changed.txt

open results/$1_qb_not_es.txt
open results/$1_es_not_qb.txt
open results/$1_changed.txt

sleep 1
rm -f crap w x y z
#rm -f $1*.txt
